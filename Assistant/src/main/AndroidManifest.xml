<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="com.assist">
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:name="com.application.AssistantApplication"
        android:allowBackup="true" android:icon="@mipmap/_logo"
        android:label="@string/app_name" android:supportsRtl="true" android:theme="@style/AppTheme">
        <activity android:name=".MainActivity" android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SelectUserActivity" android:label="@string/app_name"/>
        <activity android:name=".CommodityActivity" android:label="@string/app_name"/>
        <activity android:name=".BaskMaterialActivity" android:label="@string/app_name"/>
    </application>

</manifest>